---
layout: none
---

{
  "posts" : [
    {% for post in site.posts %}{
    	"url" : "{{ post.url }}",
    	"title" : "{{ post.title }}",
   		"author" : "{{ post.author }}",
    	"date" : "{{ post.date }}",
    	"excerpt": "{{ post.excerpt | strip_html | truncatewords: 43 }}"
    }{% unless forloop.last %},{% endunless %}
    {% endfor %}
  	]
}